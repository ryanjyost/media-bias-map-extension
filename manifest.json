{
  "manifest_version": 2,
  "name": "Media Bias Map",
  "short_name": "Media Bias Map",
  "author": "Ryan Yost",
  "description": "Rate and add items to the Media Bias Map",
  "version": "1.1.3",

  "content_scripts": [
    {
      "matches": ["http://www.google.com/*"],
      "css": ["main.css"]
    }
  ],


  "background": {
    "scripts": ["eventPage.js"],
    "persistent": true
  },

  "browser_action": {
   "default_icon": "mbm-icon.png",
   "default_popup": "popup.html"
  },

  "permissions": [
     "activeTab",
     "http://*/*",
      "https://*/*",
      "identity",
      "identity.email"
   ],

  "icons": {
          "16": "mbm-icon16.png",
           "48": "mbm-icon48.png",
          "128": "mbm-icon.png"
  },

  "key": "",

  "oauth2": {
    "client_id": "620720641935-oubnabjmlb8qnen9dsautk7tc5map7tr.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.profile "
    ]
  }
}